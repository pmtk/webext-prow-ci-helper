{
  "manifest_version": 3,
  "name": "Build logs links for prow job",
  "version": "0.1",
  "description": "TODO",
  "icons": {
    "48": "icons/prow-48.png"
  },
  "host_permissions": [
    "*://prow.ci.openshift.org/*",
    "*://gcsweb-ci.apps.ci.l2s4.p1.openshiftapps.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "*://prow.ci.openshift.org/view/gs/test-platform-results/*"
      ],
      "js": [
        "prow.js"
      ]
    }
  ],
  "page_action": {
    "default_icon": {
      "19": "icons/prow-19.png",
      "38": "icons/prow-38.png"
    },
    "default_title": "Prow",
    "default_popup": "popup/popup.html",
    "show_matches": [
      "*://prow.ci.openshift.org/view/gs/test-platform-results/*"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@example.com",
      "strict_min_version": "120.0"
    }
  }
}
